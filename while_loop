package com.company;
import java.util.Scanner;
public class loops_lec21 
{
    public static void main(String[] args) 
    {
        int i=100;
        while(i<=200)
        {                        //executes only when the condition is true
        System.out.println(i);
        i++;
        }
       System.out.println("End of while loop!");
    }
}    
