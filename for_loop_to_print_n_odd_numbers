package com.company;
import java.util.Scanner;
public class loops_lec21 
{
    public static void main(String[] args) 
    {
    System.out.println("Enter a value to print all the odd values exactly upto that value");
        Scanner scr = new Scanner(System.in) ;
        int n = scr.nextInt();
         for(int j=0;j<n;j++)
         {
            System.out.println(2*j+1);
        }

    }
}

